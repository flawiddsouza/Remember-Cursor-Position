#include <windows.h>

#define VERSION_VALUE "3.0\0"
#define VERSION_DIGITALVALUE 3, 0, 0, 0

VS_VERSION_INFO VERSIONINFO
FILEVERSION	VERSION_DIGITALVALUE
PRODUCTVERSION	VERSION_DIGITALVALUE
FILEFLAGSMASK	0x3fL
FILEFLAGS 0
FILEOS VOS_NT_WINDOWS32
FILETYPE VFT_APP
FILESUBTYPE VFT2_UNKNOWN
BEGIN
	BLOCK	"VarFileInfo"
	BEGIN
		VALUE	"Translation",	0x409,	1200
	END
	BLOCK	"StringFileInfo"
	BEGIN
		BLOCK "040904b0"
		BEGIN
			VALUE	"CompanyName",	"Flawid DSouza me@flawiddsouza.com\0"
			VALUE	"FileDescription",	"A Plugin to Remember Last Cursor Position of Opened Files for Notepad++\0"
			VALUE	"FileVersion",	VERSION_VALUE
			VALUE	"InternalName",	"RememberCursorPosition.dll\0"
			VALUE	"OriginalFilename",	"RememberCursorPosition.dll\0"
			VALUE	"ProductName",	"RememberCursorPosition\0"
			VALUE	"ProductVersion",	VERSION_VALUE
		END
	END
END
